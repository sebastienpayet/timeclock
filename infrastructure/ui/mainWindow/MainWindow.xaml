<Window x:Class="TimeClock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeClock"
        mc:Ignorable="d"
        Title="Timeclock" Height="400" Width="250" DataContext="{Binding MainViewModel, Source={StaticResource Locator}}" VerticalAlignment="Center" HorizontalAlignment="Center" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="Manual" MinWidth="250" MinHeight="389" WindowStyle="ToolWindow" ShowInTaskbar="False">
    <Grid>
        <DockPanel DockPanel.Dock="Bottom" MinWidth="250" MinHeight="408">
            <Button DockPanel.Dock="Left" Content="Load Employees" Width="200" Height="25" Command="{Binding LoadEmployeesCommand}" />
        </DockPanel>
    </Grid>
</Window>
